{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": ["plugin:react/recommended"],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": ["react", "import"],
    "rules": {
        "jsx-quotes": ["error", "prefer-single"],
        "react/prop-types": 0,
        "import/no-unresolved": 2
    }
}
